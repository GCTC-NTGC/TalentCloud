{
    "extends": "lighthouse:default",
    "settings": {
        "crawler": {
            "maxDepth": 77,
            "maxChromeInstances": 7
        },
        "onlyCategories": [
            "Accessibility"
        ],
        "onlyAudits": [
            "accesskeys",
            "aria-allowed-attr",
            "aria-required-attr",
            "aria-required-parent",
            "aria-required-childres",
            "aria-roles",
            "aria-valid-attr-value",
            "audio-caption",
            "button-name",
            "bypass",
            "color-contrast",
            "definition-list",
            "dlitem",
            "document-title",
            "duplicate-id",
            "frame-title",
            "html-has-lang",
            "html-lang-valid",
            "image-alt",
            "input-image-alt",
            "lable",
            "layout-table",
            "link-name",
            "list",
            "list-item",
            "meta-refresh",
            "meta-viewport",
            "object-alt",
            "tabindex",
            "td-headers-attr",
            "th-has-data-cells",
            "valid-lang",
            "video-caption",
            "video-description"
        ]
    }
}
