{
    "name": "gctc-ntgc/talent-cloud",
    "description": "Codebase for talent.canada.ca",
    "license": "MIT",
    "require": {
        "php": "^7.3|^7.4",
        "backpack/crud": "4.1.*",
        "bacon/bacon-qr-code": "^2.0",
        "calebporzio/parental": "^0.11.0",
        "codezero/laravel-unique-translation": "^3.0",
        "doctrine/dbal": "^2.10",
        "facade/ignition": "^2.3.6",
        "fideloper/proxy": "^4.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/date": "^4.0",
        "laravel/framework": "^8.0",
        "laravel/legacy-factories": "^1.0",
        "laravel/tinker": "^2.4",
        "laravel/ui": "^3.0",
        "mcamara/laravel-localization": "^1.3",
        "nunomaduro/collision": "^5.0",
        "pragmarx/google2fa-laravel": "^1.2",
        "pragmarx/recovery": "^0.1.0",
        "rcrowe/twigbridge": "^0.12.0",
        "spatie/laravel-translatable": "^4.1",
        "twig/twig": "^2.11"
    },
    "require-dev": {
        "backpack/generators": "^3.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "laracasts/generators": "^1.8",
        "mockery/mockery": "^1.2",
        "phpunit/php-code-coverage": "^9.0",
        "phpunit/phpunit": "^9.0",
        "sebastian/global-state": "^5.0",
        "slevomat/coding-standard": "^6.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "ide-helper": [
            "@php artisan clear-compiled",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models -N -F etc/php/ide_helper_models.php",
            "@php artisan ide-helper:meta"
        ],
        "test": "phpunit",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
