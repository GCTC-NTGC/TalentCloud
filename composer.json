{
    "name": "gctc-ntgc/talent-cloud",
    "description": "Codebase for talent.canada.ca",
    "require": {
        "php": "7.2.*",
        "dimsav/laravel-translatable": "9.*",
        "guzzlehttp/guzzle": "^6",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "jumbojett/openid-connect-php": "^0.4.1",
        "lcobucci/jwt": "3.2",
        "rcrowe/twigbridge": "^0.9.6",
        "jenssegers/date": "3.3.1",
        "mcamara/laravel-localization": "~1.3",
        "barryvdh/laravel-debugbar": "3.2.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v2.10.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "symfony/thanks": "^1.0",
        "mockery/mockery": "~1.0",
        "phpmd/phpmd" : "@stable",
        "phpunit/phpunit": "~6.0",
        "reliese/laravel": "^0.0.13",
        "slevomat/coding-standard": "~4.0",
        "squizlabs/php_codesniffer": "3.*",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": [
            "composer validate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
