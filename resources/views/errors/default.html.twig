{% extends "layouts/base-h2" %}
{% block title %}
	{{ error.title }}
{% endblock %}
{% block body %}
	{% include "common/header" with {"header": error, "hide_breadcrumbs": true} %}
	<section class="error">
		<div class="container--copy">
			<h2>
				{{ exception.getStatusCode() }}
			</h2>
			{% block error %}{% endblock %}
		</div>
	</section>
{% endblock %}
