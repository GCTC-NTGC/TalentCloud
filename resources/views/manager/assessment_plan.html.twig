{% extends "layouts/base" %}

{% block body %}

    {% include "common/goc" %}
{% include "common/alert" %}

    {% include "common/header" with { 'header': { 'title': title } } %}

    {% if display_demo_notification %}
        {% include "manager/notification" %}
    {% endif %}

    <a id="skipLink"></a>

    <div
        data-clone
        id="assessment-plan-root"
        data-job-id="{{job_id|json_encode()}}"
        data-portal="{{ portal }}"
    >
    </div>

{% endblock %}

{% block scripts %}

    <script async defer
        src="{{ mix('/js/AssessmentPlanRoot.js') }}">
    </script>

{% endblock %}
