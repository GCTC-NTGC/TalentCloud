{% extends "layouts/base" %}

{% block title %}
	{{ faq.faq_title }}
{% endblock %}

{% block body %}

	{% include "common/goc" %}
	{% include "common/alert" %}

	<header data-clone>
		<div data-c-overlay="black(80)" data-c-padding="bottom(triple)" style="background-image: url('/images/sean-pollock.jpg');">
			{% include "common/menu" %}
			<div data-c-container="large" data-c-margin="tb(triple)">
				<h1 class="logotype" data-c-font-size="h1" data-c-font-weight="600" data-c-color="white" data-c-margin="bottom(double)">{{ faq.faq_title }}</h1>
				<div data-c-alignment="left" data-c-margin="top(double)">
					{% include "common/breadcrumbs" %}
				</div>
			</div>
		</div>
	</header>

	{% if show_notification %}
		{% include "manager/notification" %}
	{% endif %}

	<a id="skipLink"></a>

	{# FAQ Sections #}

	{% include "applicant/static/credentialing_layout" %}

{% endblock %}
