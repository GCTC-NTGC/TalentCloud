{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "react-intl",
            {
                "messagesDir": "./resources/assets/js/translations/extractedMessages",
                "extractFromFormatMessageCall": true
            }
        ],
        "@babel/plugin-proposal-optional-chaining"
    ]
}
